mixin users()
    .users 
        .users__body.container 
            h2.users__title Ваш чёрный и белый список пользователей
            p.users__desc Краткое описание страницы на ресурсе - В своём стремлении повысить качество жизни, они забывают, что современная методология разработки предоставляет широкие возможности для поставленных обществом задач! 
            .users__top 
                p.users__p Все : #[span 15 исполнителей]
                p.users__p Белые: #[span  10 исполнителей]
                p.users__p Черные: #[span  5 исполнителей]
            .users__content 
                .users__row 
                    .users__item.users__item_1 
                        .users__img 
                            img(src="@img/history-img.png", alt="")
                        .users__texts 
                            p.users__subtitle Название задачи
                            p.users__text Краткое описание задачи - В своём стремлении повысить качество жизни, они забывают, 
                    .users__item.users__item_2 
                        p.users__subtitle Исполнитель и причина
                        p.users__text Петя Иванов Долго
                    .users__item.users__item_4 
                        button(type="button").users__btn Белый
